# Discord Bot Token
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# OpenAI API Key (for Bobby AI conversations)
OPENAI_API_KEY=your_openai_api_key_here

# Gemini API Key (for Magic 8-Ball fallback)
GEMINI_API_KEY=your_gemini_api_key_here

# Server Configuration
PORT=3000

# Valorant API Key (Henrik Dev API)
VALORANT_API_KEY=your_valorant_api_key_here

# GitHub Personal Access Token (optional, for changelog feature)
